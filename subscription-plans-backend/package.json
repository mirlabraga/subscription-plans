    "mongoose": "^5.12.1",
    "ts-jest": "^26.5.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
