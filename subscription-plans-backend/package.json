  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mirlabraga/subscription-plans-backend/issues"
  },
  "homepage": "https://github.com/mirlabraga/subscription-plans-backend#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/*.+(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
